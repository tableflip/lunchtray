    "menubar": "^5.2.3",
    "moment": "^2.11.1"
  },
  "devDependencies": {
    "electron-packager": "^8.7.0",
    "electron-prebuilt": "^1.4.13",
    "standard": "^10.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tableflip/lunchtray.git"
